#include<bits/stdc++.h>
#define int long long int
#define print(a) for(auto x : a) cout << x << " "; cout << endl


using namespace std;

void solve(){
    int a,b;
    cin>>a>>b;

    int count = 0;
    int count1 =0;

    if(a%3==0 || b%3==0){
        cout<<0<<endl;
    }
    else if(a%3==b%3){
        cout<<1<<endl;
    }
    else{
        cout<<2<<endl;
    }
}

int32_t main(){

    int t;
    cin>>t;

    for(int i =0;i<t;i++){
        solve();
    }
}
