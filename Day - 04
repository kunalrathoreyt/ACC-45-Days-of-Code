#include <iostream>
using namespace std;

int main() {
    int t; 
    cin>>t;
    
    for(int i =0;i<t;i++){
        int n;
        cin>>n;
        
        int a[n];
        
        int count =0;
        for(int i =0;i<n;i++){
            cin>>a[i];
            if(a[i]>=1000){
                count++;
            }
        }
        cout<<count<<endl;
        
    }
	// your code goes here
	return 0;
}
